{
    "DockerRun.DisableDockerrc": true,
    "docker.dockerComposeBuild": true,
    "docker.dockerComposeDetached": true,
    "docker.defaultRegistryPath": "",
    "docker.enableDockerComposeLanguageService": true,
    "docker.showStartPage": false,
    "docker.containers.groupBy": "Compose Project Name",
    "docker.containers.sortBy": "CreatedTime",
    "docker.images.groupBy": "Repository",
    "docker.networks.groupBy": "CreatedTime",
    "docker.volumes.groupBy": "CreatedTime",
    "python.defaultInterpreterPath": "python",
    "python.terminal.activateEnvironment": true,
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.mypyEnabled": true,
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": ["--line-length=79"],
    "python.sortImports.args": ["--profile", "black"],
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "python.testing.pytestEnabled": true,
    "python.testing.pytestArgs": ["python/tests"],
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.autoSearchPaths": true,
    "python.analysis.extraPaths": ["python"],
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/.pytest_cache": true,
        "**/.mypy_cache": true
    }
}